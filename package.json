    "smiley",
    "smileys",
    "smilies",
    "ideogram",
    "ideograms"
  ],
  "dependencies": {
    "emoji.json": "^12.1.0",
    "lodash": "^4.17.15",
    "ow": "^0.15.0"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "nyc": "^15.0.0",
    "xo": "^0.25.3"
  }
}
